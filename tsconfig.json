{
  "include": [
    "src"
  ],
  "compilerOptions": {
    "target": "es5",
    "module": "ESNext",
    "esModuleInterop": true,
    "lib": [
      "dom",
      "esnext"
    ],
    "importHelpers": true,
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "moduleResolution": "node",
    "jsx": "react",
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
